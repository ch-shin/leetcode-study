   "source": [
    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        str_x = str(x)\n",
    "        reverse_str_x = str_x[::-1]\n",
    "        for i in range(len(str_x)//2):\n",
    "            if str_x[i] != reverse_str_x[i]:\n",
    "                return False\n",
    "        return True"
   ]
  }
